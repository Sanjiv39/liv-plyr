{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "live-ply",
  "version": "0.1.0",
  "identifier": "com.live-ply.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:5008",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Live Ply",
        "width": 800,
        "height": 600,
        "resizable": true,
        "userAgent": "build-liv-ply_1.0"
      }
    ],
    "security": {
      "csp": null
    }
  },
  "plugins": {
    "http": null
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
